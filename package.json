{
    "name": "vscode-oeabl",
    "publisher": "zvg",
    "version": "0.2.3",
    "description": "OpenEdge ABL language support for VS Code.",
    "displayName": "OpenEdge ABL",
    "author": "ZaphyrVonGenevese",
    "license": "LICENSE",
    "repository": {
        "type": "git",
        "url": "https://github.com/vsobotka/vscode-abl.git"
    },
    "bugs": {
        "url": "https://github.com/vsobotka/openedge-abl-syntax/issues"
    },
    "icon": "images/progress_icon.png",
    "engines": {
        "vscode": "^1.3.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "abl",
                "aliases": [
                    "OpenEdge ABL",
                    "abl",
                    "Progress",
                    "Progress 4GL"
                ],
                "extensions": [
                    ".w",
                    ".p",
                    ".i",
                    ".cls"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "abl",
                "scopeName": "source.abl",
                "path": "./syntaxes/abl.tmLanguage"
            }
        ]
    }
}
